function total_patients()
{
	var alakuddi_total_patients = document.getElementById('paymenttotal_patients0').value;
	var karambayam_total_patients = document.getElementById('paymenttotal_patients1').value;
	var andipatti_total_patients = document.getElementById('paymenttotal_patients2').value;
	var kavarapattu_total_patients = document.getElementById('paymenttotal_patients3').value;
	var okkanadu_melaiyur_total_patients = document.getElementById('paymenttotal_patients4').value;
	var total_patients = parseInt(alakuddi_total_patients) + parseInt(karambayam_total_patients) + parseInt(andipatti_total_patients) + parseInt(kavarapattu_total_patients) + parseInt(okkanadu_melaiyur_total_patients);
	
	document.getElementById("total_patients").innerHTML=total_patients;


}

